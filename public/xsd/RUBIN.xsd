          <xs:element name="Permanent" type="xs:boolean" minOccurs="0" maxOccurs="1" />
          <xs:element name="PostOperative" type="xs:boolean" minOccurs="0" maxOccurs="1" />
          <xs:element name="SampleData" type="SampleDataType" minOccurs="0" maxOccurs="1" />
          <xs:element name="Transfer" type="xs:boolean" minOccurs="0" maxOccurs="1" />
          <xs:element name="MedProcedure" type="MedProcedureType" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="ICDCatalogType" type="CatalogueKindEnumType" minOccurs="0" maxOccurs="1" /><!-- deprecated -->

