  <xs:complexType name="FlexibleDataSetType">
    <xs:annotation>
      <xs:documentation>
        Contains FlexibleDataSetTypes a.k.a LaborMethods or Messprofile.

        Category can not be 'NONE' for FlexibleDataSets with the type 'MEASUREMENT' (invalidates)
        Category defaults to 'LABOR' for
        FlexibleDataSets with the type 'MEASUREMENT'
        Category is always set to 'NONE' for FlexibleDataSets with a different type
        than 'MEASUREMENT'

        Each LaborMethod contains a reference to a CrfTemplate for its layout information. If no CrfTemplateRef is set during import, a
        new CrfTemplate is created.

        Created: If not found by code.
        Updated: By Code.
        Skipped: Never.

        Deprecated-Fields: Name, OrganisationUnitTypeRef, FlexibleValues (use FlexibleValueComplexRefs instead)
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="StringMax" minOccurs="0" maxOccurs="1" /><!-- deprecated, remains for legacy xml files -->
      <xs:element name="Code" type="String255NotBlank" minOccurs="1" maxOccurs="1" />
      <xs:element name="NameMultilingualEntries" type="MultilingualEntryType" minOccurs="0" maxOccurs="2" />

      <xs:element name="OrganisationUnitTypeRef" type="String255" minOccurs="0" maxOccurs="1" /> <!-- deprecated -->

      <xs:element name="FlexibleValues" type="String255" minOccurs="0" maxOccurs="unbounded" /><!-- deprecated -->
      <xs:element name="FlexibleValueComplexRefs" type="FlexibleValueRefType" minOccurs="0" maxOccurs="unbounded" />

      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="ParticipantRef" type="String255" />
        <xs:element name="OrganisationUnitRef" type="String255" />
        <xs:element name="RoleRef" type="String255" />
      </xs:choice>
      <xs:element name="Systemwide" type="xs:boolean" minOccurs="0" maxOccurs="1" />
      <xs:element name="FlexibleDataSetType" type="LaborMethodTypeEnumType" minOccurs="1" maxOccurs="1" />
      <xs:element name="Category" type="LaborMethodCategoryEnumType" minOccurs="0" maxOccurs="1" />
      <xs:element name="CrfTemplateRef" type="CrfTemplateRefType" minOccurs="0" maxOccurs="1" />
      <xs:element name="Version" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Version cannot set explicit during import. It is changed during import automatically, if a FlexibleValue
            (LaborValue) has changed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SyncId" type="String255NotBlank" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

