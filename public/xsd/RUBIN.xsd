  <xs:annotation>
    <xs:documentation>
      This XSD specifies XML documents for the import/export interface of CentraXX version 3.17.1.3
    </xs:documentation>
  </xs:annotation>

  <xs:element name="CentraXXDataExchange">
    <xs:annotation>
      <xs:documentation>
        XMLs generated according to this schema may be imported into the CentraXX application. The Schema can be used to create conform
        XMLs with catalog entries (like flexible values, Organizations, blood groups, etc) or effect data entered for patients.
        Each import must have a source to identify where it comes from.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Source" type="String255NotBlank" minOccurs="1" maxOccurs="1" />
        <xs:element name="SourceVersion" type="String255" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>Read only information: The version of the exporting system. Mostly the CXX version.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ExportDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" >
          <xs:annotation>
            <xs:documentation>Read only information: The time stamp of generating this export on the source system.</xs:documentation>
          </xs:annotation></xs:element>
        <xs:element name="CatalogueData" type="CatalogueDataType" minOccurs="0" maxOccurs="1" />
        <xs:element name="EffectData" type="EffectDataType" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

